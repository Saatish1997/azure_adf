								"activity": "Get Product Count",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@greater(activity('Get Product Count').output.firstRow.ProductCount,100)",
								"type": "Expression"
							},
							"ifTrueActivities": [
								{
									"name": "Get Product Description",
